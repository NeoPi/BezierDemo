<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.neopi.bezier">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".activitys.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>
        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

    <activity android:name=".activitys.ThreeOrderActivity"/>
    <activity android:name=".activitys.PathMorphAnimertorActivity"/>
    <activity android:name=".activitys.WaveBezierActivity"/>
    <activity android:name=".activitys.PathAnimationActivity"/>
    <activity android:name=".activitys.FitBezierActivity"/>
    <activity android:name=".activitys.DragCircleActivity"/>
  </application>

</manifest>